{
    "name": "meeba-web",
    "version": "1.0.0",
    "description": "BackEnd & FrontEnd of meeba - Time Tracking",
    "main": "server.js",
    "scripts": {
        "preinstall": "node tools/nodeVersionCheck.js",
        "start-message": "babel-node tools/startMessage.js",
        "start-server": "gulp develop",
        "predev": "npm-run-all --parallel start-message",
        "start": "node server.js",
        "dev": "npm-run-all --parallel open:src lint:watch start-server ",
        "open:src": "babel-node tools/srcServer.js",
        "open:dist": "babel-node tools/distServer.js",
        "lint": "esw --fix webpack.config.* public tools --color",
        "lint:watch": "npm run lint -- --watch",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "remove-dist": "rimraf ./dist",
        "prebuild": "npm run clean-dist && npm run lint && npm run test-fe",
        "build": "babel-node tools/build.js && npm run open:dist",
        "test-fe": "jest",
        "test:CI": "babel-node tools/testCi.js",
        "test:cover": "npm run test-fe -- --coverage ",
        "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
        "test:watch": "npm run test-fe -- --watch",
        "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
        "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
        "test": "mocha"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/meeba-org/meeba-web.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/meeba-org/meeba-web/issues"
    },
    "homepage": "https://github.com/meeba-org/meeba-web#readme",
    "dependencies": {
        "autosuggest-highlight": "^3.1.1",
        "axios": "0.16.2",
        "bcryptjs": "*",
        "body-parser": "*",
        "cookie-parser": "^1.4.3",
        "express": "4.15.4",
        "express-jwt": "^5.3.0",
        "express-messages": "*",
        "express-session": "*",
        "express-validator": "*",
        "material-ui": "v1.0.0-beta.13",
        "material-ui-icons": "v1.0.0-beta.15",
        "moment": "^2.19.1",
        "mongodb": "^2.2.33",
        "mongoose": "^4.12.2",
        "object-assign": "4.1.0",
        "passport-http": "*",
        "passport-local": "*",
        "react": "15.5.4",
        "react-autosuggest": "^9.3.2",
        "react-css-modules": "4.6.0",
        "react-dom": "15.5.4",
        "react-redux": "5.0.5",
        "react-router": "~3",
        "react-router-redux": "4.0.8",
        "react-tap-event-plugin": "2.0.1",
        "redux": "3.6.0",
        "redux-thunk": "2.1.0"
    },
    "devDependencies": {
        "autoprefixer": "6.5.4",
        "babel-cli": "6.18.0",
        "babel-core": "6.20.0",
        "babel-eslint": "^8",
        "babel-jest": "18.0.0",
        "babel-loader": "6.2.10",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-react-constant-elements": "6.9.1",
        "babel-plugin-transform-react-remove-prop-types": "0.2.11",
        "babel-polyfill": "6.20.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "6.16.0",
        "babel-preset-react-hmre": "1.1.1",
        "babel-preset-stage-3": "^6.24.1",
        "browser-sync": "2.18.5",
        "chai": "^4.1.2",
        "chalk": "1.1.3",
        "connect-history-api-fallback": "1.3.0",
        "coveralls": "2.11.15",
        "css-loader": "0.26.1",
        "enzyme": "2.8.2",
        "eslint": "^4.9.0",
        "eslint-plugin-import": "2.2.0",
        "eslint-plugin-react": "6.8.0",
        "eslint-watch": "^3.1.3",
        "extract-text-webpack-plugin": "2.1.0",
        "file-loader": "0.9.0",
        "gulp-jshint": "^2.0.4",
        "gulp-nodemon": "^2.2.1",
        "html-webpack-plugin": "2.24.1",
        "identity-obj-proxy": "3.0.0",
        "jest": "18.1.0",
        "jshint": "^2.9.4",
        "json-loader": "0.5.4",
        "mocha": "^3",
        "mockdate": "2.0.1",
        "node-sass": "4.5.2",
        "npm-run-all": "3.1.2",
        "opn-cli": "3.1.0",
        "postcss-loader": "1.2.1",
        "prompt": "1.0.0",
        "prop-types": "15.5.10",
        "react-hot-loader": "^3.0.0",
        "react-test-renderer": "15.5.4",
        "redux-form": "7.0.3",
        "redux-immutable-state-invariant": "1.2.4",
        "replace": "0.3.0",
        "resolve-url-loader": "2.1.0",
        "rimraf": "2.5.4",
        "sass-loader": "6.0.2",
        "style-loader": "0.13.1",
        "supertest": "^3.0.0",
        "url-loader": "0.5.7",
        "webpack": "^3.7.1",
        "webpack-bundle-analyzer": "2.1.1",
        "webpack-dev-middleware": "1.9.0",
        "webpack-dev-server": "^2.7.1",
        "webpack-hot-middleware": "2.17.1",
        "webpack-md5-hash": "0.0.5"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy",
            "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
        }
    },
    "babel": {
        "env": {
            "development": {
                "presets": [
                    "es2015",
                    "react",
                    "react-hmre",
                    "stage-3"
                ],
                "plugins": [
                    "transform-class-properties"
                ]
            },
            "production": {
                "presets": [
                    "es2015",
                    "react",
                    "stage-3"
                ],
                "plugins": [
                    "transform-class-properties",
                    "transform-react-constant-elements",
                    "transform-react-remove-prop-types"
                ]
            },
            "test": {
                "presets": [
                    "es2015",
                    "react",
                    "stage-3"
                ]
            }
        }
    }
}
