{
    "name": "meeba-web",
    "version": "1.0.0",
    "description": "BackEnd & FrontEnd of meeba - Time Tracking",
    "main": "server.js",
    "engines": {
        "node": "8.x"
    },
    "scripts": {
        "preinstall": "node tools/nodeVersionCheck.js",
        "stats": "webpack --config ./webpack.config.prod.js --profile --json > stats.json",
        "start-message": "babel-node tools/startMessage.js",
        "start-server": "gulp develop",
        "predev": "npm-run-all --parallel start-message",
        "start": "node server.js",
        "dev": "npm-run-all --parallel open:src lint:watch start-server",
        "open:src": "babel-node tools/srcServer.js",
        "open:dist": "babel-node tools/distServer.js",
        "lint": "eslint --fix webpack.config.* public tools --color",
        "lint:watch": "npm run lint",
        "clean-dist": "npm run remove-dist && mkdir dist",
        "heroku-postbuild": "npm run build",
        "remove-dist": "rimraf ./dist",
        "prebuild": "npm run clean-dist",
        "build": "webpack --config ./webpack.config.prod.js --progress --colors",
        "test-fe": "jest",
        "test:CI": "babel-node tools/testCi.js",
        "test:cover": "npm run test-fe -- --coverage ",
        "test:cover:CI": "npm run test:CI -- --coverage && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
        "test:watch": "npm run test-fe -- --watch",
        "open:cover": "npm run test:cover && opn ./coverage/lcov-report/index.html",
        "analyze-bundle": "babel-node ./tools/analyzeBundle.js",
        "test": "mocha ./test/**/*.spec.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/meeba-org/meeba-web.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/meeba-org/meeba-web/issues"
    },
    "homepage": "https://github.com/meeba-org/meeba-web#readme",
    "dependencies": {
        "@date-io/moment": "^1.1.0",
        "@material-ui/core": "^3.9.2",
        "@material-ui/icons": "^3.0.2",
        "autoprefixer": "6.5.4",
        "autosuggest-highlight": "^3.1.1",
        "axios": "0.16.2",
        "babel-core": "6.20.0",
        "babel-loader": "^7.1.4",
        "babel-plugin-transform-react-constant-elements": "6.9.1",
        "babel-plugin-transform-react-remove-prop-types": "0.2.11",
        "babel-polyfill": "6.20.0",
        "babel-preset-react": "6.16.0",
        "babel-preset-stage-1": "^6.24.1",
        "bcryptjs": "*",
        "body-parser": "*",
        "classnames": "^2.2.6",
        "compression": "^1.7.2",
        "cookie-parser": "^1.4.3",
        "css-loader": "^0.28.11",
        "dotenv": "^4.0.0",
        "exceljs": "^1.6.2",
        "express": "4.15.4",
        "express-jwt": "^5.3.0",
        "express-sslify": "^1.2.0",
        "express-validator": "^5.3.0",
        "file-loader": "^1.1.11",
        "file-saver": "^1.3.3",
        "hard-source-webpack-plugin": "^0.9.0",
        "html-webpack-plugin": "^3.2.0",
        "jss": "^9.8.7",
        "jss-rtl": "^0.2.1",
        "material-ui-pickers": "^2.2.4",
        "moment": "^2.19.3",
        "moment-timezone": "^0.5.14",
        "mongoose": "^5.2.4",
        "natives": "^1.1.6",
        "node-sass": "^4.7.2",
        "react": "^16.3.1",
        "react-authorization": "0.0.2",
        "react-autosuggest": "^9.3.2",
        "react-css-modules": "^4.7.3",
        "react-dom": "^16.3.1",
        "react-google-maps": "^9.4.5",
        "react-jss": "^8.6.1",
        "react-markdown": "^4.0.4",
        "react-redux": "5.0.5",
        "react-router": "~3",
        "react-router-redux": "4.0.8",
        "react-select": "^2.4.2",
        "react-text-mask": "^5.1.0",
        "react-transition-group": "^2.3.1",
        "react-youtube": "^7.6.0",
        "recompose": "^0.30.0",
        "redux": "3.6.0",
        "redux-debounced": "^0.4.0",
        "redux-form": "7.0.3",
        "redux-immutable-state-invariant": "1.2.4",
        "redux-thunk": "2.1.0",
        "rimraf": "2.5.4",
        "sass-loader": "^7.0.3",
        "sass-material-colors": "0.0.5",
        "script-ext-html-webpack-plugin": "^2.0.1",
        "style-loader": "^0.21.0",
        "supertest": "^3.0.0",
        "url-loader": "^1.0.1",
        "webpack": "^4.12.0",
        "webpack-chunk-hash": "^0.6.0"
    },
    "devDependencies": {
        "babel-cli": "^6.18.0",
        "babel-eslint": "^8.2.6",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react-hmre": "1.1.1",
        "browser-sync": "2.18.5",
        "chai": "^4.1.2",
        "chalk": "1.1.3",
        "connect-history-api-fallback": "1.3.0",
        "coveralls": "2.11.15",
        "eslint": "^4.19.1",
        "eslint-plugin-import": "^2.12.0",
        "eslint-plugin-react": "^7.9.1",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "gulp-jshint": "^2.1.0",
        "gulp-nodemon": "^2.2.1",
        "jest": "18.1.0",
        "jshint": "^2.9.4",
        "mocha": "^3",
        "npm-run-all": "3.1.2",
        "opn-cli": "3.1.0",
        "postcss-loader": "^2.1.5",
        "prop-types": "15.5.10",
        "react-hot-loader": "^4.6.5",
        "redux-devtools-extension": "^2.13.8",
        "replace": "0.3.0",
        "uglifyjs-webpack-plugin": "^1.2.7",
        "webpack-bundle-analyzer": "^2.13.1",
        "webpack-cli": "^3.0.8",
        "webpack-dev-middleware": "^3.5.2",
        "webpack-dev-server": "^3.1.14",
        "webpack-hot-middleware": "^2.24.3"
    },
    "jest": {
        "moduleNameMapper": {
            "\\.(css|scss)$": "identity-obj-proxy",
            "^.+\\.(gif|ttf|eot|svg|woff|woff2|ico)$": "<rootDir>/tools/fileMock.js"
        }
    },
    "babel": {
        "env": {
            "development": {
                "presets": [
                    "env",
                    "react",
                    "stage-1"
                ],
                "plugins": [
                    "react-hot-loader/babel"
                ]
            },
            "production": {
                "presets": [
                    "env",
                    "react",
                    "stage-1"
                ],
                "plugins": [
                    "transform-react-constant-elements",
                    "transform-react-remove-prop-types"
                ]
            },
            "test": {
                "presets": [
                    "env",
                    "react",
                    "stage-1"
                ]
            }
        }
    }
}
